#! /bin/sh

# Move all device specific PATH stuff to this
if [ -f ~/.paths ]; then
  source ~/.paths
fi

# Enable completions
if [ -f $(brew --prefix)/etc/bash_completion ]; then
  . $(brew --prefix)/etc/bash_completion
fi

if [ -f ~/.aliases ]; then
  source ~/.aliases
fi

if [ -f ~/.bashrc ]; then
  source ~/.bashrc
fi
